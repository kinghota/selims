<?xml version="1.0"?>
<object name="portal_javascripts">

  <!-- jQuery -->
  <javascript
      id="++resource++senaite.lims.jquery.js/jquery-1.12.4.min.js"
      cacheable="True" compression="none" cookable="True"
      enabled="True" inline="False" expression="" bundle="default"
      insert-before="*"/>

  <!-- jQuery Migration -->
  <javascript
      id="++resource++senaite.lims.jquery.js/jquery-migrate-1.4.1.min.js"
      cacheable="True" compression="none" cookable="True"
      enabled="True" inline="False" expression="" bundle="default"
      insert-after="++resource++senaite.lims.jquery.js/jquery-1.12.4.min.js"/>

  <!-- remove builtin plone.app.jquery -->
  <javascript remove="True" id="++resource++plone.app.jquery.js"/>

  <!-- Bootstrap -->
  <javascript
      id="++resource++senaite.lims.bootstrap.vendor/js/bootstrap.min.js"
      cacheable="True" compression="none" cookable="True"
      enabled="True" inline="False" expression="" bundle="default"
      insert-after="++resource++senaite.lims.jquery.js/jquery-migrate-1.4.1.min.js"/>

  <!-- Senaite Bootstrap Integration -->
  <javascript
      id="++resource++senaite.lims.bootstrap.static/js/bootstrap-integration.js"
      cacheable="True" compression="none" cookable="True"
      enabled="True" inline="False" expression="" bundle="default"
      insert-after="*"/>

  <!-- Deactivate unneeded JS -->
  <javascript
      id="++resource++search.js"
      enabled="0"/>
  <javascript
      id="popupforms.js"
      enabled="1"/>
  <javascript
      id="modernizr.js"
      enabled="0"/>
  <javascript
      id="styleswitcher.js"
      enabled="0"/>
  <javascript
      id="dropdown.js"
      enabled="0"/>
  <javascript
      id="inline_validation.js"
      enabled="0"/>
  <javascript
      id="jquery.highlightsearchterms.js"
      enabled="0"/>
  <javascript
      id="register_function.js"
      enabled="0"/>
  <javascript
      id="select_all.js"
      enabled="0"/>
  <javascript
      id="first_input_focus.js"
      enabled="0"/>
  <javascript
      id="accessibility.js"
      enabled="0"/>
  <javascript
      id="toc.js"
      enabled="0"/>
  <javascript
      id="kss-bbb.js"
      enabled="0"/>

</object>
